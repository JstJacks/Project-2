    "# BTC\n",
    "df = pd.read_csv(btc, skiprows=[0], parse_dates = True, infer_datetime_format = True).sort_index(ascending = False)\n",
    "df = df.drop(columns = [\"unix\", \"open\",\"high\",\"low\",\"Volume BTC\", \"Volume USDT\",\"tradecount\"])\n",
    "df[\"pct change\"] = df[\"close\"].pct_change()\n",
    "\n",
    "# change datetime from object type to datetime format\n",
    "df['date'] = pd.to_datetime(df['date'])\n",
    "\n",
    "# separating datetime into separate columns\n",
    "df['Dates'] = pd.to_datetime(df['date']).dt.date\n",
    "df['Time'] = pd.to_datetime(df['date']).dt.time\n",
    "\n",
    "# dropping original date\n",
    "df = df.drop(columns = \"date\")\n",
    "\n",
    "# setting column names \n",
    "BTC = df.set_index([\"Dates\",\"Time\"]).drop(columns = \"symbol\")\n",
    "C = [\"BTC/USD Price\", \"BTC hourly change\"]\n",
    "BTC.columns = C\n",
    "BTC[\"4hrs change\"] = BTC[\"BTC/USD Price\"].pct_change(periods = 4)\n",
    "BTC[\"8hrs change\"] = BTC[\"BTC/USD Price\"].pct_change(periods = 8)\n",
    "BTC[\"12hrs change\"] = BTC[\"BTC/USD Price\"].pct_change(periods = 12)\n",
    "BTC[\"24hrs change\"] = BTC[\"BTC/USD Price\"].pct_change(periods = 24)\n",
    "BTC.head()"
   ]
  },
